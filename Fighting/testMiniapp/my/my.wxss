page {
    padding: 0rpx;
    margin: 0rpx;
}

.container {
    width: 100%;
    padding: 0rpx;
    margin: 0rpx;
}

.top {
    width: 100%;
    height: 600rpx;
    justify-content: space-between;
    align-content: space-between;
    flex-direction: column;
    position: absolute;
}

.images {
    position: absolute;
    height: 580rpx;
}

.top1 {
    height: 580rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.top2 {
    width: 100%;
    height: 300rpx;
    display: flex;
    margin-left: 60rpx;
    padding-top: 40rpx;
    position: relative;
}

.people_img {
    width: 160rpx;
    height: 135rpx;
    overflow: hidden;
    display: block;
    border-radius: 50%;
    border: 2px solid #fff;
    box-shadow: 3px 3px 10px rgba(0, 0, 0, 0.2);
}

/* .people_img open-data {
	width: 100%;
	height: 100%;
	border-radius: 90rpx;
} */

.top-main {
    width: 92%;
    margin-left: 30rpx;
    color: aliceblue;
}

.top-main image {
    width: 49rpx;
    height: 49rpx;
    margin-left: 20rpx;
}

.right {
    display: flex;
    justify-content: right;
    margin-top: 30rpx;
    margin-left: 140rpx;
}

.right image {
    width: 50rpx;
    height: 50rpx;
}

.customer {
    font-size: 20px;
    padding-top: 30rpx;
    margin-right: 65rpx;
}

.userInfo {
    margin: 16rpx 0rpx;
    display: flex;
}

.address {
    margin-left: 10rpx;
}

.userInfo .phone {
    margin-left: 50rpx;
}

.cenPage1 {
    margin: 0rpx auto;
    width: 97%;
    height: 220rpx;
    border-radius: 10rpx;
    display: flex;
    position: relative;
    margin-bottom: 40rpx;
}

.white {
    background-color: #fff;
    margin: 0rpx 20rpx;
    width: 90%;
    border-radius: 30rpx;
}

.white image {
    width: 65rpx;
    height: 65rpx;
}

.commission {
    display: flex;
    justify-content: space-between;
    padding-right: 55rpx;
}

/* .commission>image {
	width: 40rpx;
	height: 40rpx;
} */

.order {
    width: 100%;
    padding-left: 18px;
    padding-top: 10rpx;
    font-size: 31rpx;
}

.money {
    font-size: 33rpx;
    color: red;
    margin-top: 16rpx;
}

.duizhang {
    width: 100%;
    background-color: #fff;
}

.main-dui {
    width: 92%;
    margin: 16rpx auto;
    font-size: 14px;
}

.qian {
    display: flex;
    justify-content: flex-end;
    margin-left: auto;
    color: #0eb300;
}

.title {
    padding-top: 2px;
    font-size: 39rpx;
}

.kuai {
    margin-top: 18rpx;
    border-bottom: 1rpx solid gray;
}

.top-kuai {
    margin-top: 4rpx;
    padding: 20rpx;
    display: flex;
    position: absolute;
    position: relative;
}

.min-kuai {
    color: #a2a2a2;
    margin-top: 4rpx;
    padding: 20rpx;
    padding-top: 0rpx;
}

.bot-kuai {
    background-color: #eaeaea;
    margin-top: 4rpx;
    padding: 20rpx;
    display: flex;
    justify-content: flex-end;
    margin-left: auto;
    color: brown;
}
